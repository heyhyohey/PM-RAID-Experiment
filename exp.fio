[global]
name=fio 
rw=${RW}
norandommap=1 
invalidate=0 
bs=${BLOCKSIZE}
numjobs=${THREADS}
time_based=1 
clocksource=cpu 
ramp_time=30 
runtime=120 
group_reporting=1 
ioengine=${ENGINE} 
iodepth=1
fdatasync=${FDATASYNC}
direct=${DIRECT}
fsync=${FSYNC}

[pnode0]
directory=${MOUNTPOINTSOCKET0}
size=${WORKLOAD_SIZE}
file_service_type=random 
nrfiles=${FILES}
cpus_allowed=0-9
